@page
@model CleanArchi.Web.Areas.Identity.Pages.Account.Manage.UsersModel
@{
    ViewData["Title"] = "Profile";
    ViewData["ActivePage"] = ManageNavPages.Users;
}
<h1>User Roles</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Email</th>
            <th>Roles</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.UsersWithRoles)
        {
        <tr>
            <td>@user.Email</td>
            <td>@string.Join(" , ", user.Roles)</td>
            <td>
                <a class="btn btn-primary" asp-page="./ManageUser" asp-route-userId="@user.UserId">Manage Roles</a>
            </td>
        </tr>
        }
    </tbody>
</table>